---
merge-key: "{{ ACCOUNT_ID }}/{{ CLUSTER_ID }}/{{ MAS_INSTANCE_ID }}"

ibm_aibroker:
  mas_instance_id: "{{ MAS_INSTANCE_ID }}"
  aibroker_namespace: "{{ AIBROKER_NAMESPACE }}"

  mas_aibroker_watsonx_secret: "{{ MAS_AIBROKER_WATSONX_SECRET }}"
  pull_secret_name: "<path:{{ SECRETS_PATH }}:{{ IBM_ENTITLEMENT_KEY }}"

/Users/prajapatiyogeshkumatr/ARGO_REPO/Argo_odh11/cli/image/cli/mascli/templates/gitops/appset-configs/cluster/instance/ibm-aibroker.yaml.j2
#storage
  mas_aibroker_storage_provider: "{{ MAS_AIBROKER_STORAGE_PROVIDER }}"
  mas_aibroker_minio_secret: "{{ MAS_AIBROKER_MINIO_SECRET }}"
  mas_aibroker_storage_host: "<path:{{ SECRETS_PATH }}:{{ MAS_AIBROKER_STORAGE_HOST }}>"
  mas_aibroker_storage_port: "<path:{{ SECRETS_PATH }}:{{ MAS_AIBROKER_STORAGE_PORT }}>"
  mas_aibroker_storage_region: "{{ MAS_AIBROKER_STORAGE_REGION }}"
  mas_aibroker_storage_accesskey: "<path:{{ SECRETS_PATH }}:{{ MAS_AIBROKER_STORAGE_ACCESSKEY }}>"
  mas_aibroker_storage_secretkey: "<path:{{ SECRETS_PATH }}:{{ MAS_AIBROKER_STORAGE_SECRETKEY }}>"
  mas_aibroker_storage_pipelines_bucket: "<path:{{ SECRETS_PATH }}:{{ MAS_AIBROKER_STORAGE_PIPELINES_BUCKET }}>"
  mas_aibroker_storage_ssl: "{{ MAS_AIBROKER_STORAGE_SSL }}" 
  mas_aibroker_storage_tenants_bucket: "<path:{{ SECRETS_PATH }}/{{ MAS_AIBROKER_STORAGE_TENANTS_BUCKET }}>"
  mas_aibroker_storage_templates_bucket: "<path:{{ SECRETS_PATH }}/{{ MAS_AIBROKER_STORAGE_TEMPLATES_BUCKET }}>"

#Database
  mas_aibroker_db_host: "<path:{{ SECRETS_PATH }}:{{ MAS_AIBROKER_DB_HOST }}>"
  mas_aibroker_mariadb_secret: "<path:{{ SECRETS_PATH }}:{{ MAS_AIBROKER_DB_PORT }}>"
  mas_aibroker_db_port: "<path:{{ SECRETS_PATH }}:{{ MAS_AIBROKER_DB_PORT }}>"
  mas_aibroker_db_user: "<path:{{ SECRETS_PATH }}:{{ MAS_AIBROKER_DB_USER }}>"
  mas_aibroker_db_database: "<path:{{ SECRETS_PATH }}:{{ MAS_AIBROKER_DB_DATABASE }}>"
  mas_aibroker_db_secret_name: "<path:{{ SECRETS_PATH }}:{{ MAS_AIBROKER_DB_SECRET_NAME }}>"
  mas_aibroker_db_secret_value: ""<path:{{ SECRETS_PATH }}:{{ MAS_AIBROKER_DB_SECRET_VALUE }}>"

#Default Storage
  primary_storage_class: "{{ PRIMARY_STORAGE_CLASS }}"

#SLS Registration
  slscfg_registration_key: "<path:{{ SECRETS_PATH }}:{{ SLSCFG_REGISTRATION_KEY }}>"

#DRO
  drocfg_ca: "<path:{{ SECRETS_PATH }}:{{ DROCFG_CA }}>"
  drocfg_registration_key: "<path:{{ SECRETS_PATH }}:{{ DROCFG_REGISTRATION_KEY }}>"
  drocfg_url: "<path:{{ SECRETS_PATH }}:{{ DROCFG_URL }}>"

#JDBC Secret
  jdbccfg_username: "<path:{{ SECRETS_PATH }}:{{ JDBCCFG_USERNAME }}>"
  jdbccfg_password: "<path:{{ SECRETS_PATH }}:{{ JDBCCFG_PASSWORD }}>"
  jdbccfg_url: "<path:{{ SECRETS_PATH }}:{{ JDBCCFG_URL }}>"
  jdbccfg_sslenabled: "<path:{{ SECRETS_PATH }}:{{ JDBCCFG_SSLENABLED }}>"
  jdbccfg_ca: "<path:{{ SECRETS_PATH }}:{{ JDBCCFG_CA }}>"

# Development Registry Entitlement
  mas_entitlement_key: "<path:{{ SECRETS_PATH }}:{{ MAS_ENTITLEMENT_KEY }}>"
  artifactory_username: "{{ ARTIFACTORY_USERNAME }}"
  artifactory_token: "<path:{{ SECRETS_PATH }}:{{ ARTIFACTORY_TOKEN }}>"

 # Environment Variable:
  mas_app_channel: "{{ MAS_APP_CHANNEL }}"
  mas_catalog_source: "{{ MAS_CATALOG_SOURCE }}"

 # Source container registry
  mas_icr_cp: "{{ MAS_ICR_CP }}"
  mas_icr_cpopen: "{{ MAS_ICR_CPOPEN }}"

  cluster_domain: "{{ CLUSTER_DOMAIN }}"
  in_saas_env: "{{ IN_SAAS_ENV }}"
